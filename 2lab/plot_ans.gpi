#! /usr/bin/gnuplot -persist

set title "Res"

set xlabel "t" font "Helvetica Bold ,18"
set ylabel "X" rotate by 90 font "Helvetica Bold  ,18"
set zlabel "f(x,t)" font "Helvetica Bold ,18"

set xrange [0:1]
set yrange [0:pi]
set zrange [-1:1]

set view 60, 70
a=0.5

splot "./ans.txt",  sin(y - a*x)  
pause -1 "Hit enter to continue\n"
